{
    "name": "azizdevfull/lang-gen",
    "description": "Automatic translation generator for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Azizbek",
            "email": "aziz16110904@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "autoload-dev": {
        "psr-4": {
            "AzizDevFull\\LangGen\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "AzizDevFull\\LangGen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AzizDevFull\\LangGen\\LangGenServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.9",
        "phpunit/phpunit": "^12.5"
    }
}